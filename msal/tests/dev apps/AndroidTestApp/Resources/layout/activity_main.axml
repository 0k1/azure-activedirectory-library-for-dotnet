<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:minWidth="25px"
    android:minHeight="25px">
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/email" />
    <Button
        android:text="Acquire Token Interactive"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/acquireTokenInteractiveButton" />
    <Button
        android:text="Acquire Token Silent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/acquireTokenSilentButton" />
	<ToggleButton
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/useEmbeddedWebviewSwitch"
		android:layout_marginBottom="5.6dp"
		android:layout_marginTop="0.0dp"
		android:autoText="true"
		android:textOff="Use Embedded Webview" />
	<Button
        android:text="System Browser/Default Embedded Webview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/isSystemBrowserButton"
        android:layout_marginBottom="5.6dp"
        android:layout_marginTop="0.0dp" />
	<Button
        android:text="CLEAR CACHE"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/clearCacheButton"
        android:layout_marginBottom="5.6dp"
        android:layout_marginTop="0.0dp" />
    <TextView
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/accessTokenTextView"
        android:textIsSelectable="true"
        android:text="test"
        android:textStyle="bold" />
</LinearLayout>
